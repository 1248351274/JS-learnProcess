	-moz-transform:scale(1.5); 
	
阻止点击事件

//css禁用鼠标点击事件
.test { pointer-events: none; }（最好）

 e.stopPropatation||e.cancelBubble = true

 $('a.tooltip').live('click', function(event) {
    alert("抱歉,已停用！");
    event.preventDefault();
});
 $('.disableCss').removeAttr('onclick'); 